{
    "name": "delirium/http-router",
    "description": "Attribute-based HTTP Router for Delirium Framework (Swoole + PSR-7)",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Delirium\\Http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Delirium\\Http\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require": {
        "php": "^8.4",
        "ext-swoole": "*",
        "psr/http-message": "^2.0",
        "psr/http-factory": "^1.1",
        "psr/container": "^2.0",
        "nyholm/psr7": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}